{
    "leadText": {
        "title": "Procurement procedures",
        "breadcrumbs": {
            "crumbs": [
                {
                    "text": "Main",
                    "href": "/"
                },
                {
                    "text": "ETP Sections"
                },
                {
                    "text": "Sale of property",
                    "href": "/sale"
                },
                {
                    "text": "Procurement procedures"
                }
            ]
        },
        "rectLink": {
            "text": "Procurement subscription",
            "href": "/subscription"
        }
    },
    "message": {
        "text": "Attention to&nbsp;users! Register in&nbsp;advance and top up&nbsp;your personal account to&nbsp;participate in&nbsp;the procedure, as&nbsp;registration and crediting of&nbsp;funds can take up&nbsp;to&nbsp;7&nbsp;working days."
    },
    "search": {
        "searchInput": {
            "placeholder": "Enter a keyword or the procedure number",
            "submitText": "Search"
        },
        "filters": [
            {
                "title": "Organizer",
                "type": "checkbox",
                "search": {
                    "placeholder": "Enter the name of the organization or TIN"
                },
                "fields": [
                    {
                        "name": "organizer"
                    }
                ],
                "dataName": "organizers"
            },
            {
                "title": "Method of determining the supplier (purchaser)",
                "type": "checkbox",
                "fields": [
                    {
                        "name": "types"
                    }
                ],
                "dataName": "types"
            },
            {
                "title": "Statuses of procedures (stages)",
                "type": "checkbox",
                "fields": [
                    {
                        "name": "status"
                    }
                ],
                "dataName": "statuses"
            },
            {
                "title": "Price, RUB",
                "type": "price",
                "fields": [
                    {
                        "name": "sumPrice_start",
                        "label": "From"
                    },
                    {
                        "name": "sumPrice_end",
                        "label": "To"
                    }
                ]
            },
            {
                "title": "Date",
                "type": "date",
                "fields": [
                    {
                        "name": "datePublished",
                        "label": "Publication of the procedure"
                    },
                    {
                        "name": "dateEndRegistration",
                        "label": "The end of accepting applications"
                    }
                ]
            },
            {
                "title": "Type of property being sold",
                "type": "checkboxTree",
                "search": {
                    "placeholder": "Enter the name of the PS"
                },
                "fields": [
                    {
                        "name": "propertyTypeCodes"
                    }
                ],
                "dataName": "property-types"
            },
            {
                "title": "Region of the property being sold",
                "type": "radio",
                "search": {
                    "placeholder": "Enter the name or area code"
                },
                "fields": [
                    {
                        "name": "saleAreal"
                    }
                ],
                "dataName": "regions"
            },
            {
                "title": "Security",
                "type": "radio",
                "fields": [
                    {
                        "name": "deposit",
                        "data": [
                            {
                                "label": "All",
                                "value": ""
                            },
                            {
                                "label": "With collateral",
                                "value": "true"
                            },
                            {
                                "label": "Without collateral",
                                "value": "false"
                            }
                        ]
                    }
                ]
            },
            {
                "title": "Requirement of EDS for participation",
                "type": "radio",
                "fields": [
                    {
                        "name": "sign",
                        "data": [
                            {
                                "label": "All",
                                "value": ""
                            },
                            {
                                "label": "Participation without EP",
                                "value": "true"
                            },
                            {
                                "label": "Participation with the EP",
                                "value": "false"
                            }
                        ]
                    }
                ]
            },
            {
                "title": "Cost of participation",
                "type": "radio",
                "fields": [
                    {
                        "name": "noFeeCharged",
                        "data": [
                            {
                                "label": "All",
                                "value": ""
                            },
                            {
                                "label": "No fee is charged",
                                "value": "true"
                            },
                            {
                                "label": "The fee is charged",
                                "value": "false"
                            }
                        ]
                    }
                ]
            },
            {
                "title": "The Organizer's region",
                "type": "checkbox",
                "search": {
                    "placeholder": "Enter the name or area code"
                },
                "fields": [
                    {
                        "name": "regionCodes"
                    }
                ],
                "dataName": "regions"
            }
        ],
        "tags": {
            "activeIndex": "-1",
            "list": [
                {
                    "name": "sectionsCodes",
                    "value": "vitrine;sale",
                    "label": "Commercial property"
                },
                {
                    "name": "sectionsCodes",
                    "value": "sale178;les",
                    "label": "Government property"
                },
                {
                    "name": "sectionsCodes",
                    "value": "arrested_sale;sale_arrest",
                    "label": "Arrested property"
                }
            ]
        }
    },
    "sort": {
        "sortList": [
            {
                "name": "Relevance",
                "property": "actual"
            },
            {
                "name": "High procedure price",
                "property": "procedure_price_asc"
            },
            {
                "name": "Low procedure price",
                "property": "procedure_price_desc"
            },
            {
                "name": "New",
                "property": "datePublished_desc"
            },
            {
                "name": "Old",
                "property": "datePublished_asc"
            }
        ],
        "viewList": [
            {
                "title": "Block view",
                "icon": "viewCard"
            },
            {
                "title": "Table view",
                "icon": "viewTable"
            }
        ]
    }
}
