                "type": "checkbox",
                "fields": [
                    {
                        "name": "types"
                    }
                ],
                "dataName": "types"
            },
            {
                "title": "Статусы процедур (стадии)",
                "type": "checkbox",
                "fields": [
                    {
                        "name": "status"
                    }
                ],
                "dataName": "statuses"
            },
            {
                "title": "Цена, ₽",
                "type": "price",
                "fields": [
                    {
                        "name": "sumPrice_start",
